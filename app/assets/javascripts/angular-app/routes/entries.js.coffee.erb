Toptal.config ["$routeProvider", ($routeProvider) ->
  $routeProvider.when("/entries/new",
    templateUrl: "<%= asset_path('assets/angular-app/templates/entries/new.html') %>"
    controller: "EntriesNewController"
  ).when("/entries/:id/edit",
    templateUrl: "<%= asset_path('assets/angular-app/templates/entries/edit.html') %>"
    controller: "EntriesEditController"
  )
]